#   callmain.ums makes call to main to run calculator and halt when  #
#                      the program is done                           #
#                                                                    #
#####################################################################
# Registers                                                          #
#	r0: holds zero                                               #
#       r1: holds location address to return to callmain             #
#       r6: temporary register                                       #
#       r7: temporary register                                       #
######################################################################

.section init
r0 := 0
.temp r6, r7
	goto main linking r1
	halt